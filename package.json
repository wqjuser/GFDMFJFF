{
  "name": "json-to-dart-freezed",
  "displayName": "JsonToDart(Freezed)",
  "description": "Generate Freezed Dart models from JSON for Flutter projects.",
  "version": "0.1.2",
  "publisher": "wqjuser",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wqjuser/GFDMFJFF"
  },
  "icon": "images/icon.png",
  "activationEvents": [
    "workspaceContains:pubspec.yaml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jsonToDartFreezed.generate",
        "title": "JsonToDart(Freezed)",
        "category": "JsonToDart"
      },
      {
        "command": "jsonToDartFreezed.regenerate",
        "title": "Regenerate Model (JSON)",
        "category": "JsonToDart"
      },
      {
        "command": "jsonToDartFreezed.rebuildOutputs",
        "title": "Regenerate Freezed Outputs",
        "category": "JsonToDart"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "jsonToDartFreezed.generate",
          "when": "explorerResourceIsFolder && jsonToDartFreezed.isFlutterProject",
          "group": "navigation@50"
        },
        {
          "command": "jsonToDartFreezed.regenerate",
          "when": "resourceExtname == .dart && jsonToDartFreezed.isFlutterProject",
          "group": "navigation@51"
        },
        {
          "command": "jsonToDartFreezed.rebuildOutputs",
          "when": "resourceExtname == .dart && jsonToDartFreezed.isFlutterProject",
          "group": "navigation@52"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/node": "^18.19.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.3"
  }
}
